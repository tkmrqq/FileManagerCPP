QScrollBar:vertical {
    background-color: transparent;
    width: 8px;
    border-radius: 4px;
    border: 0px;
}
QScrollBar::handle:vertical {
   background: #131313;
   border-radius: 4px;
   min-height: 10px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical{ background: none;}
